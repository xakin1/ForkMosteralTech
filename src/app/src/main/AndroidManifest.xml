<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MonsteralTech.NoActionBar"
        tools:targetApi="31">
        <activity
            android:name=".ui.profile.DetailedProfileActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="sensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.home.filters.FurnitureFilterActivity"
            android:exported="false"
            android:parentActivityName=".ui.home.filters.ShowFurnitureActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.home.filters.ShowFurnitureActivity" />
        </activity>
        <activity
            android:name=".ui.home.filters.ElectronicFilterActivity"
            android:exported="false"
            android:parentActivityName=".ui.home.filters.ShowElectronicActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.home.filters.ShowElectronicActivity" />
        </activity>
        <activity
            android:name=".ui.home.filters.HouseFilterActivity"
            android:exported="false"
            android:parentActivityName=".ui.home.filters.ShowHouseActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.home.filters.ShowHouseActivity" />
        </activity>
        <activity
            android:name=".ui.home.filters.CarFilterActivity"
            android:exported="false"
            android:parentActivityName=".ui.home.filters.ShowCarActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.home.filters.ShowCarActivity" />
        </activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.facebook.sdk.ClientToken"
            android:value="@string/facebook_client_token" />

        <activity
            android:name=".ui.home.filters.ShowCarActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="sensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.home.filters.ShowElectronicActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="sensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.home.filters.ShowFurnitureActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="sensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ui.home.filters.ShowHouseActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="sensor">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation" />
        <activity
            android:name="com.facebook.CustomTabActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActionBarActivity"
            android:exported="false" />
        <activity
            android:name=".ProductDetail"
            android:exported="false"
            android:screenOrientation="sensor" />
        <activity
            android:name=".SearchActivity"
            android:exported="false"
            android:label="@string/title_activity_search"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.MonsteralTech.NoActionBar" />
        <activity
            android:name=".FavoritesActivity"
            android:exported="false"
            android:label="@string/title_activity_favorites"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.MonsteralTech.NoActionBar" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false"
            android:screenOrientation="sensor" />
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:screenOrientation="sensor"
            android:theme="@style/Theme.MonsteralTech.NoActionBar" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:exported="true"
            android:screenOrientation="sensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>